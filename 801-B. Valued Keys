#include <bits/stdc++.h>
using namespace std;
 
int main()
{   
    int flag;
    string x,y;
    cin>>x;
    cin>>y;
    
    for(int i=0;i<x.length();i++)
    {
        if(y[i]-x[i]>0)
        {
          flag=1;
          break;
        }
        else
        {
            flag=0;
        }
    }
    
   if(flag==1)
   {
       cout<<"-1"<<endl;
   }
   else
   {
       for(int i=0;i<x.length();i++)
       {
           cout<<y[i];
       }
       cout<<endl;
   }
    return 0;
}
